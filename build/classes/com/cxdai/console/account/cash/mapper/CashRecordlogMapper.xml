<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.cxdai.console.account.cash.mapper.CashRecordlogMapper">
	
	<!-- 插入记录，返回主键 -->
	<insert id="insertCashRecordlog" parameterType="com.cxdai.console.account.cash.entity.CashRecordlog">
		INSERT INTO t_cashrecord_log 
		(CASHRECORD_ID, 	TYPE,	ADDTIME,	ADDIP,	ADD_USERID,	    PLATFORM)
		VALUES
		(#{cashrecordId},#{type},#{addtime},#{addip},#{addUserId},#{platform})
		
 		 <selectKey resultType="java.lang.Integer" order="AFTER" keyProperty="id">  
      	    SELECT LAST_INSERT_ID() AS ID    
   		 </selectKey>
	</insert>
	
</mapper>  